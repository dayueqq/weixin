var dataJson=[{"act_id":"52c57deb91f20","company_id":"36","act_name":"\u7f6e\u6362\u5b9d\u9a6c5\u7cfb\u6b63\u5728\u4f18\u60e0","path":"..\/..\/..\/common\/image\/36\/act\/1388674539.JPG","actDescribe":"<p>2013\u5e7412\u670823\u65e5-2014\u5e7401\u670821\u65e5\uff0c \u4e1c\u839e\u5b9d\u4fe1\u5b9d\u9a6c\u7f6e\u6362\u5b9d\u9a6c5\u7cfb\u6b63\u5728\u4f18\u60e0\uff0c\u611f\u5174\u8da3\u7684\u670b\u53cb\u53ef\u4ee5\u5230\u5e97\u54a8\u8be2\u8d2d\u4e70\u3002<\/p>","startTime":"2013-12-23","endTime":"2014-01-21"},{"act_id":"52c57ec8e9acc","company_id":"36","act_name":"\u4e1c\u839e\u5b9d\u4fe1 BMW X1\u8d2d\u8f66\u6709\u8c6a\u793c\uff0c\u6b32\u8d2d\u4ece\u901f","path":"..\/..\/..\/common\/image\/36\/act\/1388674760.jpg","actDescribe":"<p>&nbsp; &nbsp; \u4e1c\u839e\u5b9d\u4fe1\u5373\u65e5\u8d77\u8d2d\u4e70BMW X1\u5373\u9001\u4ef7\u503c\u4e07\u5143\u7684\u51ac\u65e5\u5927\u793c\u5305\uff08BMW\u539f\u5382\u592a\u9633\u819c+\u8f66\u9876\u884c\u674e\u67b6\uff09\uff0c\u8ba9\u60a8\u51fa\u6e38\u6fc0\u60c5\u4f9d\u65e7\u3002<\/p><p><strong>360\u00b0\u91d1\u878d\u65b9\u6848<\/strong><\/p><p>&nbsp; &nbsp; \u5168\u65b9\u4f4d\u7545\u4eab100%\u81ea\u7531\uff012013\u5e7411\u67081\u65e5\u81f312\u670831\u65e5\uff0c\u9996\u4ed850%\u8d2dBMW X1\uff0c\u8d37\u6b3e12\u4e2a\u6708\uff0c\u6bcf\u6708\u6700\u9ad8\u8fd8\u6b3e\u4ec5360\u5143\u3002\u8d37\u6b3e\u671f\u672b\uff0c\u60a8\u53ef\u4ee5\u4e00\u6b21\u6027\u4ed8\u6e0550%\u5f39\u6027\u4f59\u6b3e\uff0c\u4e5f\u53ef\u4ee5\u5bf9\u5176\u8fdb\u884c\u5c55\u671f\uff0c\u7ee7\u7eed\u4eab\u53d7BMW\u91d1\u878d\u4e13\u4e1a\u670d\u52a1\u3002<\/p><p><strong>\u60a6\u4eab99\u91d1\u878d\u65b9\u6848<\/strong><\/p><p>&nbsp; &nbsp; 2013\u5e7411\u67081\u65e5\u81f312\u670831\u65e5\uff0c\u8d2d\u8f66\u53ef\u201c99\u53cc\u4eab\u65b9\u6848\u201d\uff1a\u9996\u4ed830\uff05\u8d77\uff0c3.99\uff05\u8d77\u4f4e\u5229\u7387\uff0c12-48\u4e2a\u6708\u591a\u79cd\u8d37\u6b3e\u671f\u9650\u4f9b\u9009\u3002<br\/><\/p><p>&nbsp; &nbsp; BMW X1\u90fd\u80fd\u4f7f\u60a8\u4eab\u53d7\u5230\u7edd\u5bf9\u7